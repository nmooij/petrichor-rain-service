{% extends "base.xml" %}
{% block content %}

<form id="input_form">

  <field name="DTMF_input" type="digits?minlength=1;maxlength=5">
    <prompt>
     <audio src="{{ ask_input_label }}"/>
    </prompt>

    <filled>
    
     <assign name="DTMF_input" expr="DTMF_input"/>
    
      <submit src=" {{redirect_url}}" namelist="DTMF_input" method="post"/> 
    </filled>
  </field>
</form>

{% endblock %}
